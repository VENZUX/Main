local RunService = game:GetService("RunService")

local objectsToDestroy = {
    "EffectPart",
    "PickupTag",
    "Model",
    "BigMeteorBall",
    "RadialIndicator",
    "Fireball",
    "PoisonBall",
    "BlastIndicator", 
    "Part",
    "IceSpikeRing",
    "AggroPoint",
    "IceScratch",
    "PoisonBeam",
    "TreeEntSpike",
    "RockSpikeRing",
    "MediumCrystal",
    "DireDeathballPink",
    "CommanderSwordPrisonSword",
    "SpikeShockwave",
    "Shockball",
    "MeteorSpecialSmall",
    "MeteorSpecialMedium",
    "FireBase",
    "Whirlpool",
    "WaterBeam",
    ""
}

local function destroyObjects()
    for _, objectName in ipairs(objectsToDestroy) do
        local object = workspace:FindFirstChild(objectName)
        if object then
            object:Destroy()
        end
    end
end

local function onUpdate()
    destroyObjects()
end

RunService.Stepped:Connect(onUpdate)
