local RunService = game:GetService("RunService")

local objectsToDestroy = {
    "EffectPart",
    "PickupTag",
    "BigMeteorBall",
    "RadialIndicator",
    "Fireball",
    "PoisonBall",
    "BlastIndicator", 
    "IceSpikeRing",
    "AggroPoint",
    "IceScratch",
    "PoisonBeam",
    "TreeEntSpike",
    "RockSpikeRing",
    "MediumCrystal",
    "DireDeathballPink",
    "CommanderSwordPrisonSword",
    "SpikeShockwave",
    "Shockball",
    "MeteorSpecialSmall",
    "MeteorSpecialMedium",
    "FireBase",
    "Whirlpool",
    "WaterBeam",
    "BubbleBall",
    "ConeIndicator",
    "LightningPanel",
    "Lightning1",
    "Lightning2",
    "BlueFireCylinder",
    "DarkFireBeam",
    "LighteningSegment",
    "HadesFireWall",
    "HadesSpike",
    "GuardianSwordPrisonSwordPurple",
    "PurpleMeteorSpecialMedium",
    "FallenKingSpike",
    "FireballPurple",
    "PoisonCylinder",
    "HadesFireWallPurple",
    ""
}

local function destroyObjects()
    for _, objectName in ipairs(objectsToDestroy) do
        local object = workspace:FindFirstChild(objectName)
        if object then
            object:Destroy()
        end
    end
end

local function onUpdate()
    destroyObjects()
end

RunService.Stepped:Connect(onUpdate)
